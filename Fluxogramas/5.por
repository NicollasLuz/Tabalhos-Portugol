programa {
  funcao inicio() {
    inteiro peso
    escreva("Bom dia João, bão?")
    escreva("Qual o peso do peixe da vez João?: ")
    leia(peso)

    se (peso > 50){
        escreva("Pô João, esse é dos bão em!!! Mas infelizmente esses ai tem a multinha né. Aí fica R$", (peso - 50) * 4, ".00 de taxa, valeu. Até mais!")
    }
    senao{
        escreva("Esse ai passou direto viu João, valeu. Até mais!")
    }
  }
}
