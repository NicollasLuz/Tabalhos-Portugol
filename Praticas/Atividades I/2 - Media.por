programa {
  funcao inicio() {
    inteiro n1, n2, n3, n4, media

    escreva("Qual a primeira nota: ")
    leia(n1)
    escreva("Qual a segunda nota: ")
    leia(n2)
    escreva("Qual a terceira nota: ")
    leia(n3)
    escreva("Qual a querta nota: ")
    leia(n4)

    media = (n1 + n2 + n3 + n4) / 4

    escreva("A média final das suas 4 notas foi: ", media)
  }
}
